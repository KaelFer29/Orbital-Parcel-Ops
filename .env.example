# Backend environment variables
DATABASE_URL=postgresql://postgres:password@localhost:5432/orbital_parcel_ops

# AWS (para desarrollo local con LocalStack u otros)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key
